<idea-plugin package="com.intellij">
  <extensions defaultExtensionNs="com.intellij">
    <project.extras implementation="com.intellij.openapi.roots.JavaLangLevelProjectExtras"/>

    <virtualFile.extras.consumer implementation="com.intellij.openapi.roots.JavaLangLevelVirtualFileExtrasConsumer"/>
    <projectService serviceImplementation="com.intellij.openapi.roots.JavaLevelPerVirtualFileHolder"/>
  </extensions>
</idea-plugin>
